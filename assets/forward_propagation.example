const inputs = nj.array([
    [0, 0],
    [0, 1],
    [1, 0],
    [1, 1]
])

const hidden_size = 3

let weights_0_1 = nj.random([2, hidden_size]).multiply(2).subtract(1)
let weights_1_2 = nj.random([hidden_size, 1]).multiply(2).subtract(1)

for (let iteration = 0; iteration < 1; iteration++) {
    for (let i = 0; i < inputs.shape[0]; i++) {
        let layer_0 = inputs.slice([i, i + 1])
        let layer_1 = relu(nj.dot(layer_0, weights_0_1))
        let outputLayer = nj.dot(layer_1, weights_1_2)
    }
}